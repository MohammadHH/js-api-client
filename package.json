{
  "name": "@typeform/api-client",
  "version": "0.1.0-alpha.1",
  "description": "JS SDK for Typeform API",
  "main": "lib/typeform.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepser/typeform-js-sdk.git"
  },
  "keywords": [
    "sdk",
    "typeform",
    "js"
  ],
  "author": "Jepser Bernardino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jepser/typeform-js-sdk/issues"
  },
  "homepage": "https://github.com/jepser/typeform-js-sdk#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "in-publish": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "sinon": "^4.5.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "TYPEFORM_TOKEN=$TYPEFORM_TOKEN jest",
    "test:unit": "jest ./tests/unit",
    "test:unit:watch": "jest ./tests/unit --watch",
    "test:integration": "TYPEFORM_TOKEN=$TYPEFORM_TOKEN jest ./tests/integration/",
    "test:integration:watch": "TYPEFORM_TOKEN=$TYPEFORM_TOKEN jest ./tests/integration/ --watch",
    "build:dist": "webpack --mode production",
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "prepublish": "in-publish && npm run build:dist && npm run build:lib || not-in-publish",
    "pretty": "prettier './{src,tests}/**/*.js' --write"
  },
  "peerDependencies": {}
}
